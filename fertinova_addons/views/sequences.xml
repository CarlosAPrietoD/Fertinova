<odoo>
    <data noupdate="1">

        <!--::::::::::::::::::::::::::::::::::::::::::::-->
        <!-- SEQUENCE for name of Waste Stock Moves     -->
        <!--::::::::::::::::::::::::::::::::::::::::::::-->
        <record id="sequence_wobin_stock_waste" model="ir.sequence">
        <field name="name">Wobin Waste Stock Moves</field>
        <field name="code">self.wastestock</field>
        <field name="active">TRUE</field>
        <field name="prefix">GC_MER</field>
        <field name="padding">6</field>
        <!--<field name="number_next">1</field>-->
        <field name="number_increment">1</field>
        </record> 

        <!--::::::::::::::::::::::::::::::::::::::::::::-->
        <!-- SEQUENCE for name of Surplus Stock Moves   -->
        <!--::::::::::::::::::::::::::::::::::::::::::::-->
        <record id="sequence_wobin_stock_waste" model="ir.sequence">
        <field name="name">Wobin Surplus Stock Moves</field>
        <field name="code">self.surplustock</field>
        <field name="active">TRUE</field>
        <field name="prefix">GC_EXC</field>
        <field name="padding">6</field>
        <!--<field name="number_next">1</field>-->
        <field name="number_increment">1</field>
        </record>             
    
    </data>  
</odoo>      