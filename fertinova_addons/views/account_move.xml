<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <record id="account_move_check" model="ir.ui.view">
            <field name="name">account.move.check</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <!--<xpath expr="//field[@name='journal_id']" position="after">
                    <field name="prov"/>
                </xpath>-->
                <xpath expr="//button" position="after">
                    <button name="duplicate_account" type="object" string="Duplicate Inverse" class="oe_highlight"/>
                </xpath>
                <xpath expr="//tree/field[@name='account_id']" position="before">
                    <field name="purchase"/>
                    <field name="po_status"/>
                    <field name="po_date"/>
                    <field name="sale"/>
                    <field name="so_status"/>
                    <field name="so_date"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>   